	<defs>
		<style type="text/css"><![CDATA[
			polygon { fill: green;
				  stroke: black;
				  stroke-width: 1} 
			]]>
		</style>
		<m:canton id="infoOf_10001001" bndtype="village">
			<m:name lang="tonyonPinyin" desc="myvillage"/>
			<m:statistics>
				<m:population count="100"/>
			</m:statistics>
			<m:geometricInfo area="6400" circumference="320"/>
		</m:canton>
	</defs>

